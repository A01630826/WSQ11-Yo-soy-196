def reverse(x) :
    x = str(x)
    x = int(x[::-1])
    return x
def is_palindrome(x) :
    for i in range(30) :
        if x == reverse(x) and i == 0 :
            return "Natural Palindrome"
        elif x == reverse(x) :
            return "Non-Lycherel"
        else :
            x += reverse(x)
    else :
        return "Lycherel number candidates"
		
a = int(input("Ingrese un numero (entero) para iniciar: "))
b = int(input("Ingrese el valor mÃ¡s alto: "))

if a < b :
    for i in range(a, b + 1) :
        print (i, is_palindrome(i))
if b > a :
    for i in range(b, a + 1) :
        print (i, is_palindrome(i))
else :
    print (a, is_palindrome(a))
